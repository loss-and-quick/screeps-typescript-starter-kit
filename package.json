{
  "name": "screeps-typescript-starter-kit",
  "version": "0.9.0",
  "description": "Begin your journey of building impressive projects in Screeps:World with this typescript starter kit.",
  "main": "dist/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "@minicx",
  "license": "Unlicense",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rollup -c",
    "push:main": "rollup -c --environment DEST:main",
    "push:season": "rollup -c --environment DEST:season",
    "push:sim": "rollup -c --environment DEST:sim",
    "watch:main": "rollup -cw --environment DEST:main",
    "watch:season": "rollup -cw --environment DEST:season",
    "watch:sim": "rollup -cw --environment DEST:sim"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/screeps": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "git-rev-sync": "^3.0.2",
    "path": "^0.12.7",
    "prettier": "^2.8.6",
    "rollup": "^3.20.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-screeps": "^1.0.1",
    "rollup-plugin-screepy": "^3.0.6",
    "typescript": "^5.0.2"
  }
}